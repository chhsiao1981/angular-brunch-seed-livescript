mod = {}

mod.{{project_camelcase_uppercase}}Ctrl = <[$scope $location $resource $rootScope]> ++ (s, $location, $resource, $rootScope) ->
  s <<< {$location}

  s.$watch '$location.path()' (activeNavId or '/') ->
    s <<< {activeNavId}

  s.getClass = (id) ->
    if (s.activeNavId.substring 0 id.length) is id then 'active' else ''

angular.module '{{project}}.controllers' <[{{project}}.services]> .controller mod